{
  "name": "blog",
  "version": "0.3.9",
  "description": "my personal blog",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf docs/*",
    "start": "npm run clean && cross-env ELEVENTY_ENV=dev eleventy --serve",
    "build": "npm run clean && cross-env ELEVENTY_ENV=prod eleventy",
    "commit": "git add -A && git commit",
    "push": "git push && git push --tags",
    "publish": "npm run build && npm run commit && npm version patch && npm run push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joaomelo/blog.git"
  },
  "keywords": [
    "blog"
  ],
  "author": "joaomelo",
  "license": "CC-BY-SA-4.0",
  "bugs": {
    "url": "https://github.com/joaomelo/blog/issues"
  },
  "homepage": "https://github.com/joaomelo/blog#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "@toycode/markdown-it-class": "^1.2.4",
    "cross-env": "^7.0.2",
    "moment": "^2.26.0",
    "rimraf": "^3.0.2"
  },
  "dependencies": {}
}
